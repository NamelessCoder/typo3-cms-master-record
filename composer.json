{
    "name": "namelesscoder/typo3-cms-master-record",
    "description": "Test provider extension for FluidTYPO3",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0+",
    "autoload": {
        "psr-4": {
            "NamelessCoder\\MasterRecord\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NamelessCoder\\MasterRecord\\Tests": "Tests/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "typo3/cms-core": ">=7.6.13, <7.6.99|^8.4.1|^9|^10|dev-master"
    },
    "replace": {
        "master_record": "self.version"
    }
}
